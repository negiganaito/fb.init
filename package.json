    "@tanstack/react-query": "^5.35.1",
    "@tanstack/react-query-devtools": "5.35.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/jest": "^29.5.12",
